{
    "name": "mara",
    "tree": {
      "$className": "DataModel",

      "Players": {
        "$className": "Players",
        "$properties": {
          "CharacterAutoLoads": false
        }
      },

      "ReplicatedStorage": {
        "$className": "ReplicatedStorage",
        "Packages": {
            "$path": "Packages",
            "Mara": {
                "$path": "lib"
            }
        },

        "DevPackages": {
            "$path": "DevPackages"
          }
      },

      "ServerScriptService": {
        "$className": "ServerScriptService",
        "Benchmarks":{
          "$path": "test/benchmarks"
        }
      },

      "TestService": {
        "$className": "TestService",
        "$properties": {
          "ExecuteWithStudioRun": true
        },

        "Test": {
          "$path": "test/runTests.server.luau"
        }
      }
    }
  }